<table class="table table-striped table-hover ">
<thead>
  <tr>
    <th>Title</th>
    <th>Artist</th>
    <th>Play<th>
    <th></th>
  </tr>
</thead>
<tbody>
  <% @songs.each do |song| %>
  <tr class="active">
    <td><%= song.title %></td>
    <td><%= song.artist %></td>
    <td> <%= audio_tag(song.audio, controls: true) %></td>
    <td>
      <%= link_to 'Delete', song, class: 'btn btn-danger',
          method: :delete, data: { confirm: 'Are you sure?' } if admin_user? %>
    </td>
  </tr>
  <% end %>
  </tbody>
  </table>